//Filmlerin detay sayfasına gitmek için
void GotoDetay
{
    //film idlerini göndermek için
    gonder = ListView.clSelectedItemData(clText5); 
    
    Clomosy.GlobalVariableString = gonder; 
    
    uAnaSayfa.clShow;
    uDetay.UnitName = 'Detay';
    uDetay.CallerForm = uAnaSayfa;
    uDetay.Run;
}

//Fragman Film sayfasına gitmek için
void GotoFragmanliFilm
{
    uAnaSayfa.clShow;
    uFragmanliFilm.UnitName = 'FragmanliFilm';
    uFragmanliFilm.CallerForm = uAnaSayfa;
    uFragmanliFilm.Run;
}

