  searchEdt.Align = alTop;
  searchEdt.Margins.Top = 5;
  searchEdt.Margins.Left = 5;
  searchEdt.Margins.Right = 5;
  searchEdt.Margins.Bottom = 5;
  searchEdt.Height = 40;
}

{
