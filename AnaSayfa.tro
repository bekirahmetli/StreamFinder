//listview oluşturma
void CreateListView
{
  ListView = uAnaSayfa.AddNewProListView(uAnaSayfa,'ListView');
  ListView.Align = AlClient;
  ListView.Height = 170;
  ListView.Margins.Bottom = 5;
  ListView.Margins.Top = 5;
  ListView.Margins.Right = 5;
  ListView.Margins.Left = 5;
  ListView.clProSettings.ViewType = lvIcon;
  ListView.clProSettings.ColCount = 1;
  ListView.clProSettings.ItemHeight = 100;
  ListView.clProSettings.ItemSpace = 10;
  ListView.clProSettings.BackgroundColor = clAlphaColor.clHexToColor('#FFFCFF');
  ListView.clProSettings.BorderColor = clAlphaColor.clHexToColor('#66FF7F');
  ListView.clProSettings.BorderWidth = 2;
  ListView.clProSettings.RoundHeight = 5;
  ListView.clProSettings.RoundWidth = 5;
  ListView.SetclProSettings(ListView.clProSettings);
  
  searchEdt.TargetListView = ListView;
}

void CreateDesignerPanel
{
  DesignerPanel = uAnaSayfa.AddNewProListViewDesignerPanel(ListView, 'DesignerPanel');
  DesignerPanel.Height = 100;
  DesignerPanel.Width = 150;
  DesignerPanel.clProSettings.BackgroundColor = clAlphaColor.clHexToColor('#B3FCE5');
  DesignerPanel.clProSettings.BorderColor = clAlphaColor.clHexToColor('#16FF8C');
  DesignerPanel.clProSettings.BorderWidth = 2;
  DesignerPanel.clProSettings.RoundHeight = 20;
  DesignerPanel.clProSettings.RoundWidth = 20;
  DesignerPanel.SetclProSettings(DesignerPanel.clProSettings);
  
  ListView.SetDesignerPanel(DesignerPanel);
}

//Jsondaki verileri ListViewede nereye koyulacağını işaret eden fonksiyon
void CreateData;
{  
  Poster = uAnaSayfa.AddNewProImage(DesignerPanel, 'Poster');
  Poster.Align = AlLeft;
  Poster.Margins.Top = 10;
  Poster.Margins.Bottom = 10;
  DesignerPanel.AddPanelObject(Poster, clImage1);

  Title = uAnaSayfa.AddNewProLabel(DesignerPanel, 'Title', '');
  Title.Align = AlVertCenter;
  Title.clProSettings.AutoSize=True;
  Title.Height =30;
  Title.Margins.Bottom = 10;
  Title.Margins.Top = 10;
  Title.Margins.Right = 10;
  Title.Margins.Left = 10;
  Title.clProSettings.FontColor = clAlphaColor.clHexToColor('#000000');
  Title.clProSettings.FontSize = 9;
  Title.clProSettings.AutoSize=True;
  Title.SetclProSettings(Title.clProSettings);
  DesignerPanel.AddPanelObject(Title, clText);
    
  Year = uAnaSayfa.AddNewProLabel(DesignerPanel, 'Year', '');
  Year.Align = AlVertCenter;
  Year.Margins.Top = 55;
  Year.Margins.Right = 10;
  Year.Margins.Left = 10;
  Year.clProSettings.FontColor = clAlphaColor.clHexToColor('#000000');
  Year.clProSettings.FontSize = 15;
  Year.clProSettings.AutoSize=True;
  Year.SetclProSettings(Year.clProSettings);
  DesignerPanel.AddPanelObject(Year, clText2);
  
  imdbID = uAnaSayfa.AddNewProLabel(DesignerPanel, 'imdbID', '');
  DesignerPanel.AddPanelObject(imdbID, clText5); 
  imdbID.Visible = false;
  
  
  Type1 = uAnaSayfa.AddNewProLabel(DesignerPanel, 'Type', '');
  Type1.Align = AlVertCenter;
  Type1.Margins.Top = 20;
  Type1.Margins.Right = 10;
  Type1.Margins.Left = 10;
  Type1.clProSettings.FontColor = clAlphaColor.clHexToColor('#000000');
  Type1.clProSettings.FontSize = 15;
  DesignerPanel.AddPanelObject(Type1, clText3);  
   
  uAnaSayfa.AddNewEvent(ListView,tbeOnItemClick,'GotoDetay');
}

